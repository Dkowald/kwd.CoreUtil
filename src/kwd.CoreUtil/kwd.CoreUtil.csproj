<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>1.0.0</Version>
    <Authors>Derek Kowald</Authors>
    <Product>KWD Core Utilities</Product>
    <Title>KWD Core Utilities</Title>
    <Description>A set of extensions to provvide a better development experiance for .NET Core</Description>
    <PackageProjectUrl>https://github.com/Dkowald/kwd.CoreUtil</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Dkowald/kwd.CoreUtil.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <Tags>.NET Core;Utility</Tags>
		<PackageReleaseNotes>
      ## New set of helpers for Streams.
      Tee for writing to multiple streams at once.

      Helpers for reading / writing lines.
		</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Include="icon.png" Pack="true" PackagePath="\" />
    <None Include="icon.svg" Pack="false" />
  </ItemGroup>

</Project>
